{
  "fields": [{
      "name": "facility_id",
      "title": "Facility ID",
      "type": "string",
      "constraints": {
        "required": true
      }
    },
    {
      "name": "sex",
      "title": "Sex",
      "type": "string",
      "constraints": {
        "required": true,
        "enum": ["male", "female"]
      }
    }, {
      "name": "age_start",
      "title": "Age Start",
      "type": "number",
      "decimalChar": ".",
      "constraints": {
        "required": true,
        "minimum": 0,
        "maximum": 130
      }
    },
    {
      "name": "age_span",
      "title": "Age Span",
      "type": "number",
      "decimalChar": ".",
      "constraints": {
        "required": true,
        "minimum": 1,
        "maximum": 130
      }
    }, {
      "name": "on_art",
      "title": "Number on ART",
      "type": "integer",
      "constraints": {
        "required": true,
        "minimum": 0
      }
    }, {
      "name": "new_clients",
      "title": "Number New Clients",
      "type": "integer",
      "constraints": {
        "required": true,
        "minimum": 0
      }
    }, {
      "name": "died",
      "title": "Number Died",
      "type": "integer",
      "constraints": {
        "required": true,
        "minimum": 0
      }
    }, {
      "name": "disengaged",
      "title": "Number Disengaged",
      "type": "integer",
      "decimalChar": ".",
      "constraints": {
        "required": true,
        "minimum": 0
      }
    }
  ],
  "primaryKey": ["facility_id", "sex", "age_start", "age_span"],
  "foreignKeys": [{
    "fields": "facility_id",
    "reference": {
      "resource": "unaids_geographic_facilities",
      "fields": "facility_id"
    }
  }],
  "custom-constraint": {
    "constraint": "on_art >= new_clients",
    "constraint": "on_art >= died",
    "constraint": "on_art >= disengaged"
  }
}
