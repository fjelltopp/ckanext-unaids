{
  "fields": [{
      "name": "facility_id",
      "title": "Facility ID",
      "type": "string",
      "constraints": {
        "required": true
      }
    },
    {
      "name": "age_group_id",
      "title": "Age Group ID",
      "type": "integer",
      "constraints": {
        "required": true
      }
    },
    {
      "name": "quarter_id",
      "title": "Quarter ID",
      "type": "integer",
      "constraints": {
        "required": true
      }
    },
    {
      "name": "anc_clients",
      "title": "Number ANC clients",
      "type": "integer",
      "constraints": {
        "required": true,
        "minimum": 0
      }
    },
    {
      "name": "ancrt_hiv_status",
      "title": "ANCRT HIV Status",
      "type": "integer",
      "constraints": {
        "required": true,
        "minimum": 0
      }
    },
    {
      "name": "ancrt_known_pos",
      "title": "Number known positive",
      "type": "integer",
      "constraints": {
        "required": true,
        "minimum": 0
      }
    },
    {
      "name": "ancrt_already_art",
      "title": "Number already on ART",
      "type": "integer",
      "constraints": {
        "required": true,
        "minimum": 0
      }
    },
    {
      "name": "ancrt_tested",
      "title": "Number HIV tested",
      "type": "integer",
      "constraints": {
        "required": true,
        "minimum": 0
      }
    },
    {
      "name": "ancrt_test_pos",
      "title": "Number HIV Tested Positive",
      "type": "integer",
      "constraints": {
        "required": true,
        "minimum": 0
      }
    }
  ],
  "primaryKey": ["facility_id", "quarter_id", "age_group_id"],
  "foreignKeys": [{
    "fields": "facility_id",
    "reference": {
      "resource": "unaids_geographic_facilities",
      "fields": "facility_id"
    }
  }],
  "custom-constraint": {
      "constraint": "clients >= known_positive",
      "constraint": "clients >= hiv_tested",
      "constraint": "clients >= hiv_positive",
      "constraint": "clients >= already_art"
  }
}
