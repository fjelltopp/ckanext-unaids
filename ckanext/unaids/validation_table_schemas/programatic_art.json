{
  "fields": [{
      "name": "facility-id",
      "title": "Facility ID",
      "type": "string",
      "constraints": {
        "required": true
      }
    },
    {
      "name": "period-start-date",
      "title": "Period Start Date",
      "type": "date",
      "constraints": {
        "required": true,
        "minimum": "1900-01-01",
        "maximum": "2030-01-01",
        "date-format": "YYYY-MM-DD"
      }
    },
    {
      "name": "period-end-date",
      "title": "Period End Date",
      "type": "date",
      "constraints": {
        "required": true,
        "minimum": "1900-01-01",
        "maximum": "2030-01-01",
        "date-format": "YYYY-MM-DD"
      }
    },
    {
      "name": "sex",
      "title": "Sex",
      "type": "string",
      "constraints": {
        "required": true,
        "enum": ["male", "female"]
      }
    }, {
      "name": "age-start",
      "title": "Age Start",
      "type": "number",
      "decimalChar": ".",
      "constraints": {
        "required": true,
        "minimum": 0,
        "maximum": 130
      }
    },
    {
      "name": "age-span",
      "title": "Age Span",
      "type": "number",
      "decimalChar": ".",
      "constraints": {
        "required": true,
        "minimum": 1,
        "maximum": 130
      }
    }, {
      "name": "on-art",
      "title": "Number on ART",
      "type": "integer",
      "constraints": {
        "required": true,
        "minimum": 0
      }
    }, {
      "name": "new-clients",
      "title": "Number New Clients",
      "type": "integer",
      "constraints": {
        "required": true,
        "minimum": 0
      }
    }, {
      "name": "died",
      "title": "Number Died",
      "type": "integer",
      "constraints": {
        "required": true,
        "minimum": 0
      }
    }, {
      "name": "disengaged",
      "title": "Number Disengaged",
      "type": "integer",
      "decimalChar": ".",
      "constraints": {
        "required": true,
        "minimum": 0
      }
    }
  ],
  "primaryKey": ["facility-id", "sex", "age-start", "period-end-date"],
  "foreignKeys": [{
    "fields": "facility-id",
    "reference": {
      "resource": "unaids_geographic_facilities",
      "fields": "facility-id"
    }
  }],
  "custom-constraint": {
    "constraint": "on-art >= new-clients",
    "constraint": "on-art >= died",
    "constraint": "on-art >= disengaged"
  }
}
